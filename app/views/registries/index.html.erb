      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @registries.each do |registry| %>
      <tr>
        <td><%= registry.id %></td>
        <td><%= registry.codigo %></td>
        <td><%= registry.edad %></td>
        <td><%= registry.genero %></td>
        <td><%= registry.etilogia %></td>
        <td><%= registry.frac_eyeccion %></td>
        <td><%= registry.imc %></td>
        <td><%= registry.hemoglobina %></td>
        <td><%= registry.creatinina %></td>
        <td><%= registry.bnp %></td>
        <td><%= registry.pro_bnp %></td>
        <td><%= registry.eac %></td>
        <td><%= registry.diabetes %></td>
        <td><%= registry.irc %></td>
        <td><%= registry.epoc %></td>
        <td><%= registry.acv %></td>
        <td><%= registry.fa %></td>
        <td><%= registry.pas_mmhg %></td>
        <td><%= registry.pad_mmhg %></td>
        <td><%= registry.fc %></td>
        <td><%= registry.clase_funcional_nyha %></td>
        <td><%= registry.betabloqueador %></td>
        <td><%= registry.anticoagulacion %></td>
