      <th><%= model_class.human_attribute_name(:diureticos_adherido) %></th>
      <th><%= model_class.human_attribute_name(:diureticos_titulacion) %></th>
      <th><%= model_class.human_attribute_name(:ivabradina_actual) %></th>
      <th><%= model_class.human_attribute_name(:ivabradina_adherido) %></th>
      <th><%= model_class.human_attribute_name(:ivabradina_titulacion) %></th>
      <th><%= model_class.human_attribute_name(:visita_seguimiento) %></th>
      <th><%= model_class.human_attribute_name(:encargado_visita) %></th>
      <th><%= model_class.human_attribute_name(:dias_despues_alta) %></th>
      <th><%= model_class.human_attribute_name(:llenado_formato_autoseguimiento) %></th>
