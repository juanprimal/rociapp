        <td><%= registry.ara_ii %></td>
        <td><%= registry.mra %></td>
        <td><%= registry.diureticos %></td>
        <td><%= registry.ivabradina %></td>
        <td><%= registry.material_educativo %></td>
        <td><%= registry.charla_informativa %></td>
        <td><%= registry.programa_seguimiento %></td>
        <td><%= registry.encargado_segiumiento %></td>
        <td><%= registry.formato_autoseguramiento_paciente %></td>
        <td><%= registry.instalo_app %></td>
