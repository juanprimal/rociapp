        <td><%= registry.betabloqueador_adherido %></td>
        <td><%= registry.betabloqueador_titulacion %></td>
        <td><%= registry.anticoagulacion_indicada %></td>
        <td><%= registry.anticoagulacion_adherido %></td>
        <td><%= registry.iecas_actual %></td>
        <td><%= registry.iecas_adherido %></td>
        <td><%= registry.iecas_titulacion %></td>
        <td><%= registry.araii_actual %></td>
        <td><%= registry.araii_adherido %></td>
        <td><%= registry.araii_titulacion %></td>
        <td><%= registry.mra_actual %></td>
        <td><%= registry.mra_adherido %></td>
        <td><%= registry.mra_titulacion %></td>
        <td><%= registry.diureticos_actual %></td>
        <td><%= registry.diureticos_adherido %></td>
        <td><%= registry.diureticos_titulacion %></td>
        <td><%= registry.ivabradina_actual %></td>
        <td><%= registry.ivabradina_adherido %></td>
        <td><%= registry.ivabradina_titulacion %></td>
        <td><%= registry.visita_seguimiento %></td>
        <td><%= registry.encargado_visita %></td>
        <td><%= registry.dias_despues_alta %></td>
        <td><%= registry.llenado_formato_autoseguimiento %></td>
        <td><%=l registry.created_at, format: :long %></td>
        <td>
          <%= link_to registry_path(registry), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to registry_path(registry), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_registry_path,
            :class => 'btn btn-primary' %>
