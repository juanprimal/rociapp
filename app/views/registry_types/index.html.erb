<h1>Listing registry_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registry_types.each do |registry_type| %>
      <tr>
        <td><%= registry_type.name %></td>
        <td><%= link_to 'Show', registry_type %></td>
        <td><%= link_to 'Edit', edit_registry_type_path(registry_type) %></td>
        <td><%= link_to 'Destroy', registry_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registry type', new_registry_type_path %>
